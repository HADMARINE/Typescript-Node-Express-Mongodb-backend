{
  "presets": ["@babel/preset-typescript", "@babel/preset-env"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@src": "./src",
          "@models": "./src/models",
          "@lib": "./src/lib",
          "@error": "./src/error",
          "@util": "./src/util"
        }
      }
    ],
    "@babel/plugin-proposal-class-properties"
  ]
}
